  "license": "MIT",
  "private": false,
  "devDependencies": {
    "@types/node": "^15.6.1",
    "@typescript-eslint/eslint-plugin": "^4.28.0",
    "@typescript-eslint/parser": "^4.28.0",
    "eslint": "^7.29.0",
    "eslint-plugin-security": "^1.4.0",
    "jest": "^27.0.5",
    "joi": "^17.4.0",
    "typedoc": "^0.21.2",
